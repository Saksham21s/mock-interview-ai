{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "installCommand": "npm install --force --legacy-peer-deps",
        "nodeVersion": "16.x"
      }
    }
  ],
  "routes": [
    { "src": "/api/.*", "dest": "/api", "continue": true },
    { "src": "/(.*)", "dest": "/", "status": 200 }
  ]
}